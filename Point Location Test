#include <iostream>
using namespace std;

string positionRelativeToLine(int x1, int y1, int x2, int y2, int x3, int y3) {
    long long crossProduct = (x3 - x1) * (y2 - y1) - (y3 - y1) * (x2 - x1);
    if (crossProduct > 0) {
        return "LEFT";
    } else if (crossProduct < 0) {
        return "RIGHT";
    } else {
        return "TOUCH";
    }
}

int main() {
    int t;
    cin >> t;

    for (int i = 0; i < t; i++) {
        int x1, y1, x2, y2, x3, y3;
        cin >> x1 >> y1 >> x2 >> y2 >> x3 >> y3;
        cout << positionRelativeToLine(x1, y1, x2, y2, x3, y3) << endl;
    }

    return 0;
}
