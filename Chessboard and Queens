#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

int row[8] = {0};
int col[8] = {0};
int diag1[15] = {0};
int diag2[15] = {0};
int ans = 0;

bool isSafe(int i, int j) {
    return (col[j] == 0 && diag1[i + j] == 0 && diag2[i - j + 7] == 0);
}

void placeQueen(int i, vector<vector<char>>& vec) {
    if (i == 8) {
        ans++;
        return;
    }
    for (int j = 0; j < 8; j++) {
        if (isSafe(i, j) && vec[i][j] == '.') {
            col[j] = diag1[i + j] = diag2[i - j + 7] = 1;
            placeQueen(i + 1, vec);
            col[j] = diag1[i + j] = diag2[i - j + 7] = 0;
        }
    }
}

void solve() {
    vector<vector<char>> vec(8, vector<char>(8, '.'));
    
    for (auto& i : vec)
        for (auto& j : i) cin >> j;

    placeQueen(0, vec);
    cout << ans << endl;
}

int main() {
    solve();
    return 0;
}

